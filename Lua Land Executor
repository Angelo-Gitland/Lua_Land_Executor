local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
screenGui.Name = "LuaLandExecutor"
screenGui.ResetOnSpawn = false

local executorFrame = Instance.new("Frame", screenGui)
executorFrame.Name = "ExecutorFrame"
executorFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
executorFrame.Size = UDim2.new(0, 400, 0, 300)
executorFrame.Position = UDim2.new(0.5, -200, 0.5, -150)
executorFrame.Active = true
executorFrame.Draggable = true

local title = Instance.new("TextLabel", executorFrame)
title.Text = "Lua Land | Executor"
title.Size = UDim2.new(1, 0, 0, 30)
title.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
title.TextColor3 = Color3.fromRGB(0, 255, 0)
title.Font = Enum.Font.Code
title.TextSize = 18

local closeBtn = Instance.new("TextButton", executorFrame)
closeBtn.Text = "X"
closeBtn.Size = UDim2.new(0, 30, 0, 30)
closeBtn.Position = UDim2.new(1, -30, 0, 0)
closeBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
closeBtn.TextColor3 = Color3.fromRGB(0, 255, 0)
closeBtn.Font = Enum.Font.Code
closeBtn.TextSize = 18

local scrollFrame = Instance.new("ScrollingFrame", executorFrame)
scrollFrame.Size = UDim2.new(1, -20, 0, 180)
scrollFrame.Position = UDim2.new(0, 10, 0, 40)
scrollFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
scrollFrame.ScrollBarThickness = 6
scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 180)
scrollFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
scrollFrame.VerticalScrollBarInset = Enum.ScrollBarInset.ScrollBar

local textbox = Instance.new("TextBox", scrollFrame)
textbox.Size = UDim2.new(1, -10, 0, 180)
textbox.Position = UDim2.new(0, 5, 0, 0)
textbox.BackgroundTransparency = 1
textbox.TextColor3 = Color3.fromRGB(0, 255, 0)
textbox.Font = Enum.Font.Code
textbox.TextSize = 16
textbox.Text = ""
textbox.ClearTextOnFocus = false
textbox.MultiLine = true
textbox.TextWrapped = false
textbox.TextXAlignment = Enum.TextXAlignment.Center
textbox.TextYAlignment = Enum.TextYAlignment.Center

local placeholder = Instance.new("TextLabel", textbox)
placeholder.Text = "-- Enter Script"
placeholder.Size = UDim2.new(1, 0, 1, 0)
placeholder.Position = UDim2.new(0, 0, 0, 0)
placeholder.BackgroundTransparency = 1
placeholder.TextColor3 = Color3.fromRGB(0, 255, 0)
placeholder.Font = Enum.Font.Code
placeholder.TextSize = 16
placeholder.TextXAlignment = Enum.TextXAlignment.Center
placeholder.TextYAlignment = Enum.TextYAlignment.Center

textbox:GetPropertyChangedSignal("Text"):Connect(function()
	placeholder.Visible = textbox.Text == ""
end)

local buttonWidth = 100
local spacing = (400 - (buttonWidth * 3)) / 4

local executeBtn = Instance.new("TextButton", executorFrame)
executeBtn.Name = "Execute"
executeBtn.Text = "Execute"
executeBtn.Size = UDim2.new(0, buttonWidth, 0, 30)
executeBtn.Position = UDim2.new(0, spacing, 1, -40)
executeBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
executeBtn.TextColor3 = Color3.fromRGB(0, 255, 0)
executeBtn.Font = Enum.Font.Code
executeBtn.TextSize = 16

local clearBtn = Instance.new("TextButton", executorFrame)
clearBtn.Name = "Clear"
clearBtn.Text = "Clear"
clearBtn.Size = UDim2.new(0, buttonWidth, 0, 30)
clearBtn.Position = UDim2.new(0, spacing * 2 + buttonWidth, 1, -40)
clearBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
clearBtn.TextColor3 = Color3.fromRGB(0, 255, 0)
clearBtn.Font = Enum.Font.Code
clearBtn.TextSize = 16

local copyBtn = Instance.new("TextButton", executorFrame)
copyBtn.Name = "Copy"
copyBtn.Text = "Copy"
copyBtn.Size = UDim2.new(0, buttonWidth, 0, 30)
copyBtn.Position = UDim2.new(0, spacing * 3 + buttonWidth * 2, 1, -40)
copyBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
copyBtn.TextColor3 = Color3.fromRGB(0, 255, 0)
copyBtn.Font = Enum.Font.Code
copyBtn.TextSize = 16

local openBtn = Instance.new("TextButton", screenGui)
openBtn.Text = "Open"
openBtn.Size = UDim2.new(0, 100, 0, 40)
openBtn.Position = UDim2.new(0.5, -50, 0.9, -20)
openBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
openBtn.TextColor3 = Color3.fromRGB(0, 255, 0)
openBtn.Font = Enum.Font.Code
openBtn.TextSize = 18
openBtn.Visible = false

executeBtn.MouseButton1Click:Connect(function()
	local code = textbox.Text
	if code and code ~= "" then
		loadstring(code)()
	end
end)

clearBtn.MouseButton1Click:Connect(function()
	textbox.Text = ""
end)

copyBtn.MouseButton1Click:Connect(function()
	if setclipboard then
		setclipboard(textbox.Text)
	end
end)

closeBtn.MouseButton1Click:Connect(function()
	executorFrame.Visible = false
	openBtn.Visible = true
end)

openBtn.MouseButton1Click:Connect(function()
	executorFrame.Visible = true
	openBtn.Visible = false
end)
